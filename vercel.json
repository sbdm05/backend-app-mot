{
  "builds": [
    { "src": "app.js", "use": "@vercel/node" },
    { "src": "reset-password/(.*)", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/v1/letters", "dest": "/app.js" },
    { "src": "/reset-password/(.*)", "dest": "https://mes-lettres.vercel.app/reset-password/index.html" }
  ]
}


